{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,

    "isolatedModules": true,
    "noEmit": true,

    "strict": true,
    "resolveJsonModule": true,
    "noFallthroughCasesInSwitch": true
  }
}